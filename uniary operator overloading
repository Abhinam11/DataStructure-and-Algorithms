#include <iostream>

using namespace std;
class complex{
    private:
    int a,b;
    public:
    void set_data(int x,int y){
        a=x; b=y;
    }
    void display(){
        cout<<"a is:"<<a<<" "<<"b is :"<<b;
    }
    complex operator -(){
        complex temp;
        temp.a=-a;
        temp.b=-b;
        return temp;
    }
};
int main(){
    complex c1,c2;
    c1.set_data(3,4);
    
    c2=-c1;//c2=c1.operator-
    c2.display();
    return 0;
    
}
